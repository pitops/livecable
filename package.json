  "license": "MIT",
  "dependencies": {
    "chokidar": "^2.0.4",
    "event-stream": "^4.0.1",
    "express": "^4.16.3",
    "express-ws": "^4.0.0",
    "minimist": "^1.2.0",
    "send": "^0.16.2"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.13.0",
