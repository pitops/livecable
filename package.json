{
  "name": "livecable",
  "version": "0.0.12",
  "description": "A small browser agnostic live-reload library that does not require manual client-side code.",
  "main": "index.js",
  "bin": {
    "livecable": "bin/index.js"
  },
  "scripts": {
    "start": "livecable --path static --index index.html",
